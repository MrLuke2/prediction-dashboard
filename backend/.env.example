# Server
PORT=3001
NODE_ENV=development

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/alpha_predict

# Redis
REDIS_URL=redis://localhost:6379

# Auth — Access Token Secret (min 32 chars)
JWT_SECRET=
# Auth — Refresh Token Secret (min 32 chars, MUST differ from JWT_SECRET)
REFRESH_SECRET=
# Optional: rotation key — set this BEFORE rotating JWT_SECRET
NEXT_JWT_SECRET=

# Encryption — AES-256-GCM key for API key storage (min 32 chars)
ENCRYPTION_KEY=

# Platform AI Provider Keys (used when users don't BYOK)
DEFAULT_AI_PROVIDER=anthropic
DEFAULT_AI_MODEL=claude-sonnet-4-5
ANTHROPIC_API_KEY=
OPENAI_API_KEY=
GEMINI_API_KEY=

# Trading
PAPER_TRADING=true
MAX_POSITION_USD=1000
MIN_CONFIDENCE_THRESHOLD=60
ARB_THRESHOLD=0.03
WHALE_THRESHOLD_USD=10000

# AI Router
ENABLE_AI_FALLBACK=true
DAILY_COST_LIMIT_USD=5.00

# Market Data
POLYMARKET_API_KEY=
KALSHI_API_KEY=
KALSHI_API_SECRET=